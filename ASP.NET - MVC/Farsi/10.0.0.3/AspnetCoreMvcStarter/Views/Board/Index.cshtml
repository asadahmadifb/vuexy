@model IEnumerable<AspnetCoreMvcStarter.Models.CrowdFunding.BoardData>

<h2>Board Data</h2>

<table class="table table-striped table-bordered text-center">
	<thead>
		<tr>
			<th>عنوان</th>
			<th>مشتری</th>
			<th>بودجه</th>
			<th>تاریخ شروع</th>
			<th>تاریخ اتمام</th>
			<th>توضیحات</th>
			<th>تمام ساعات</th>
			<th>وظیفه</th>
			<th>نرخ تکمیل</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Title</td>
				<td>@item.Client</td>
				<td>@item.Budget</td>
				<td>@item.StartDate</td>
				<td>@item.EndDate</td>
				<td>@item.Description</td>
				<td>@item.TotalHours</td>
				<td>@item.TaskCount</td>
				<td>@item.CompletionRate</td>
			</tr>
		}
	</tbody>
</table>
