<!-- Views/BusinessPlans/Index.cshtml -->
@model List<AspnetCoreMvcStarter.Models.CrowdFunding.BusinessPlan>

<!DOCTYPE html>
<html lang="fa">
<head>
	<meta charset="utf-8" />
	<title>Business Plans</title>
</head>
<body>
	<h1>بررسی صفحه اصلی هم آفرین</h1>
	<table class="table table-striped table-bordered text-center">
		<thead class="thead-dark">
			<tr>
				<th>عنوان</th>
				<th>پیش‌بینی‌میزان‌سود</th>
@* 				<th>اطلاعات مالی</th>
 *@				<th>حداقل و حداکثر سرمایه گذاری شخص حقیقی</th>
				<th> تاریخ شروع</th>
				<th>مبلغ هدف</th>
				<th>سرمایه گذاران</th>
				<th> پیش بینی سود</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var plan in Model)
			{
				<tr>
					<td>@plan.Title</td>
@* 					<td>@plan.ProfitPrediction</td>
 *@					<td>@plan.p1</td>
					<td>@plan.p3</td>
					<td>@plan.p4</td>
					<td>@plan.GoalAmount</td>
					<td>@plan.InvestorsCount</td>
					<td>@plan.ProfitForecast</td>
				</tr>
			}
		</tbody>
	</table>
</body>
</html>
